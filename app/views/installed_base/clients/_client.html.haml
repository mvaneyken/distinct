%tr
  %td= link_to client.code, installed_base_client_path(client)
  %td= client.name
  %td
    =link_to "Edit", edit_installed_base_client_path(client), class: 'btn btn-mini'
    =link_to "Delete", installed_base_client_path(client), method: :delete, confirm: "Really delete?", class: 'btn btn-mini btn-danger'
